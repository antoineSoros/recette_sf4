{% extends 'base.html.twig' %}


{% block stylesheets %}<link rel="stylesheet" type="text/css" href="../semantic/dist/semantic.min.css">
<script
  src="https://code.jquery.com/jquery-3.1.1.min.js"
  integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
  crossorigin="anonymous"></script>
<script src="semantic/dist/semantic.min.js"></script>{% endblock %}


{% block body %}
    <h2>Filtrer</h2>
    {{ form(myForm) }}
    <h1 class="ui header">Liste des Ingredients Alpha</h1>
    <table class="ui celled table">
         <thead>
    <tr><th>Titre</th>
  
    <th>Action</th>
  </tr></thead>
         <tbody>
{% for ingredient in mesIngredients %}
    <tr><td><a href="{{ path("ingredient-focus",{"id":ingredient.id})}}">{{ ingredient.nom }}</a></td>
        
   
    <td> <a class="ui button" href="{{ path('sup-ingredient', {'id': ingredient.id}) }}">Delete</a></td></tr>
{% endfor %}
         </tbody>
    </table>
{% endblock %}
